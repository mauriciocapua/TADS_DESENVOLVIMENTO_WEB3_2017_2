package negocio;

import java.util.ArrayList;

public class Livro {
	String codigo;
	String linguagem;
	ArrayList<String> titulo;
	String publicacao;
	String editora;
	int quantidadeDeAutores;
	ArrayList<String> autores;
	String formato;
	String capa;
	
	public Livro() {
		
	}
	
	public Livro(String codigo, String linguagem, ArrayList<String> titulo, String publicacao, String editora,
			int quantidadeDeAutores, ArrayList<String> autores, String formato, String capa) {
		super();
		this.codigo = codigo;
		this.linguagem = linguagem;
		this.titulo = titulo;
		this.publicacao = publicacao;
		this.editora = editora;
		this.quantidadeDeAutores = quantidadeDeAutores;
		this.autores = autores;
		this.formato = formato;
		this.capa = capa;
	}
	
	
}
